import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int sum=0;
        for(int i=0;i<n;i++)
        {
            int value=in.nextInt();
            sum+=value;
        }
        float mean=(float)sum/n;
        System.out.printf("Array Mean Value is %.2f\n",mean);
    }
}
