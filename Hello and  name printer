import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        String b=in.nextLine();
        System.out.print("Hello");
        System.out.print("\n");
        System.out.print(b);
    }
}
