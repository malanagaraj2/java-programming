import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int a=in.nextInt();
        float b=in.nextFloat();
        System.out.println(a);
        System.out.printf("%.2f",b);
        
    }
        
}
